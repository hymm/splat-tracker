{
  "name": "squid-tracks",
  "description": "alternative splatnet2 client with ability to upload to stat.ink",
  "productName": "SquidTracks",
  "version": "0.2.0",
  "private": true,
  "author": "@Wrong_Shoe",
  "devDependencies": {
    "electron": "^1.4.14",
    "electron-builder": "^19.19.1",
    "foreman": "^2.0.0",
    "react-scripts": "^1.0.10"
  },
  "dependencies": {
    "base64url": "^2.0.0",
    "bootstrap": "3",
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.7",
    "electron-updater": "^2.8.5",
    "history": "^4.6.3",
    "husky": "^0.14.3",
    "jsonwebtoken": "^7.4.1",
    "jws": "^3.1.4",
    "lint-staged": "^4.0.2",
    "prettier": "^1.5.3",
    "promise-memoize": "^1.2.0",
    "proptypes": "^1.1.0",
    "react": "^15.4.2",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.4.2",
    "react-json-tree": "^0.10.9",
    "react-json-view": "^1.11.4",
    "react-router-bootstrap": "^0.24.2",
    "react-router-dom": "^4.1.2",
    "request-promise-native": "^1.0.4",
    "universal-analytics": "^0.4.15",
    "uuid": "^3.1.0"
  },
  "homepage": "./",
  "main": "./public/electron.js",
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prepack": "yarn build",
    "pack": "build --em.main=build/electron.js",
    "release": "build --em.main=build/electron.js --publish=always",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "dev": "nf start -p 3000"
  },
  "build": {
    "appId": "com.shoesoft.splat-track",
    "directories": {
      "buildResources": "public"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "license": "MIT"
}

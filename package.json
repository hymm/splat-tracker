{
  "name": "squid-tracks",
  "description": "alternative splatnet2 client with ability to upload to stat.ink",
  "productName": "SquidTracks",
  "version": "0.2.4",
  "private": true,
  "author": "@Wrong_Shoe",
  "devDependencies": {
    "babel-plugin-react-intl": "^2.3.1",
    "electron": "^1.4.14",
    "electron-builder": "^19.24.1",
    "foreman": "^2.0.0",
    "react-intl-translations-manager": "^5.0.0",
    "react-scripts": "^1.0.11"
  },
  "dependencies": {
    "babel-preset-electron": "^1.4.15",
    "babel-register": "^6.26.0",
    "base64url": "^2.0.0",
    "bootstrap": "3",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^3.5.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.7",
    "electron-unhandled": "^0.2.0",
    "electron-updater": "^2.8.7",
    "flat": "^4.0.0",
    "history": "^4.7.2",
    "husky": "^0.14.3",
    "json5": "^0.5.1",
    "jsonwebtoken": "^7.4.3",
    "jws": "^3.1.4",
    "lint-staged": "^4.0.4",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "msgpack-lite": "^0.1.26",
    "piping": "^1.0.0-rc.4",
    "prettier": "^1.5.3",
    "promise-memoize": "^1.2.0",
    "proptypes": "^1.1.0",
    "react": "^15.4.2",
    "react-app-rewired": "^1.2.2",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.4.2",
    "react-intl": "^2.3.0",
    "react-json-tree": "^0.10.9",
    "react-json-view": "^1.12.0",
    "react-polygon": "^0.1.0",
    "react-router-bootstrap": "^0.24.2",
    "react-router-dom": "^4.2.0",
    "recharts": "^1.0.0-alpha.4",
    "request-promise-native": "^1.0.4",
    "universal-analytics": "^0.4.15",
    "uuid": "^3.1.0",
    "wait-on": "^2.0.2"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "precommit": "lint-staged",
    "react-start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "prepack": "yarn build",
    "pack": "build --em.main=build/electron.js",
    "release": "build --em.main=build/electron.js --publish=always",
    "test": "react-app-rewired test --env=jsdom",
    "test:main": "jest -c=jest.main.config.json --watch",
    "eject": "react-app-rewired eject",
    "electron": "electron .",
    "electron-dev": "node public/main/dev.js .",
    "dev": "concurrently \"yarn react-start\" \"wait-on http://localhost:3000/ && yarn electron-dev\"",
    "manage:translations": "node ./translationRunner.js"
  },
  "build": {
    "appId": "com.shoesoft.splat-track",
    "directories": {
      "buildResources": "public"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "public/main/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "license": "MIT"
}
